#include<stdio.h>

int main() {
    int a,b,c,d,f,g;
    printf("enter first number:");
    scanf("%d", &a);
    printf("enter second number:");
    scanf("%d", &b);
    c=a+b;
    d=a-b;
    f=a*b;
    g=a/b;
    printf("addition %d\n", c);
    printf("subtract %d\n", d);
    printf("multiply %d\n", f);
    printf("divide %d\n", g);
    return 0;
}
